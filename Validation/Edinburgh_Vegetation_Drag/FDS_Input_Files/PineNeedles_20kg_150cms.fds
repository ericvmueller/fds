&DEVC XB=0.025,0.025,0.0,0.0,0.0025,0.0975,POINTS=20,QUANTITY='U-VELOCITY',ID='U3_rms',
STATISTICS_START=20,STATISTICS_END=80,TEMPORAL_STATISTIC='RMS'/
