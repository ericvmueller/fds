&HEAD CHID='PineNeedles_20kg_150cms',  TITLE='test of velocity profile in and above layer of pine needles' /

&MESH IJK=40,30,20, XB=-0.1,0.1,-0.075,0.075,0.0,0.10, MULT_ID='mesh_mult'/
&MULT ID='mesh_mult', DX=0.2, I_UPPER=3/


&TIME T_END=80/

&RADI RADIATION=.FALSE./

&MISC TMPA=20.0/

&VENT PBX=-0.1, SURF_ID='INLET' /
&VENT PBX=0.7, SURF_ID='OPEN' /

&SURF ID='INLET',VEL=-1.5,PROFILE='RAMP',RAMP_V_Z='inlet-ramp'/

&SURF ID                        = 'foliage'
      MATL_ID                   = 'DRY VEGETATION'
      SURFACE_VOLUME_RATIO      = 4661
      LENGTH                    = 0.12
      N_LAYER_CELLS_MAX         = 1
      GEOMETRY                  = 'CYLINDRICAL'/ 
&MATL ID = 'DRY VEGETATION'
      DENSITY = 615.
      CONDUCTIVITY = 0.1
      SPECIFIC_HEAT = 2.0/

&PART ID='pine needles', DRAG_LAW='POROUS MEDIA', SAMPLING_FACTOR=1, SURF_ID='foliage',
STATIC=.TRUE.,COLOR='GOLD',DRAG_COEFFICIENT=0.03,0.03,0.03,
PERMEABILITY=3.63E-06,3.63E-06,3.63E-06,POROUS_VOLUME_FRACTION=0.0325/
&INIT ID='bed',PART_ID='pine needles', XB=0.0,0.3,-.075,0.075,0.0,.05, N_PARTICLES_PER_CELL=1, 
MASS_PER_VOLUME=20,CELL_CENTERED=.TRUE./

&DUMP DT_DEVC=0.1,DT_PART=1000,DT_SLCF=.1/

&DEVC XB=-0.075,-0.075,0.0,0.0,0.0025,0.0975,POINTS=20,QUANTITY='U-VELOCITY',ID='U1',
STATISTICS_START=20,STATISTICS_END=80,TEMPORAL_STATISTIC='TIME AVERAGE'/
&DEVC XB=-0.075,-0.075,0.0,0.0,0.0025,0.0975,POINTS=20,QUANTITY='U-VELOCITY',ID='U1_rms',
STATISTICS_START=20,STATISTICS_END=80,TEMPORAL_STATISTIC='RMS'/
